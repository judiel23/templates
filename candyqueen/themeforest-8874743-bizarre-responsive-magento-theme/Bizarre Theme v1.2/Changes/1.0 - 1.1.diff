+/* Custom Widgets Option */
+.title{
+	width: 313px;
+	float: left;
+}
+.title-2{
+	width: 255px;
+	padding-left: 40px;
+	float: left;
+}
+.title h2,
+.title-2 h2{
+	font-size: 16px;
+}
+.widget-content{
+	padding:10px;
+	width: 293px;
+	float: left;
+	background-color:#FFF;
+}
+.widget-content .item{
+	clear: both;
+	display: block;
+	float: left;
+	position: relative;
+	cursor: pointer;
+	-moz-transition:opacity 300ms ease;
+	-webkit-transition:opacity 300ms ease;
+	-o-transition:opacity 300ms ease; 
+	transition:opacity 300ms ease;
+}
+.widget-content .item:hover{
+	opacity: 0.3;
+	filter: alpha(opacity=30);
+	position: relative;
+}
+
+.widget-content .item.inline{
+	float: none;
+	clear: none;
+	display: inline-block;
+}
+.widget-holder{clear: both;}
+
+.widget-holder .img-box{position: relative; padding-bottom: 18px;}
+.widget-holder .img-box .item{position: absolute; width: 50%; height: 15px; padding:8px 0 7px; text-align: center; bottom: 5px;}
+.widget-holder .img-box.only-one .item{width: 100%;}
+.widget-holder .img-box .item.quickview-item{left: 0;}
+.widget-holder .item.quickview-item{background:#f6c48d;}
+.widget-holder .img-box .item.moreviews-item{right: 0;}
+.widget-holder .item.moreviews-item{background:#e0ae77;}
+
+.widget-holder h2.product_name{
+	font-size: 14px;
+	font-weight: 400;
+	line-height: 1.583em;
+	color:#222;
+	position: relative;
+	margin-bottom: 0;
+}
+.widget-holder .close.product_name{right: -9px; top: -7px;}
+.widget-holder .reviews{
+	color: #aaaaaa;
+	font-size:11px;
+	line-height: 1.2em;
+	text-decoration: none;
+	float: left;
+	margin-top: 3px;
+}
+.widget-holder .rating_stars{margin-right: 10px; margin-top: 5px;}
+.widget-holder .close.rating{right: 0;}
+.widget-holder .review-divide{padding:0 2px;}
+.widget-content .left-col{float: left;}
+.widget-content .cart-btn{float: right;}
+.widget-holder .price{
+	clear: both;
+	position: relative;
+}
+.widget-holder .price .regular-price{
+	float: left;
+	font-size: 14px;
+	font-family: Georgia, sans-serif;
+	font-weight: 300;
+	line-height: 1.583em;
+	text-decoration: line-through;
+	color:#da2c2a;
+	padding:0 5px 0 0;
+}
+.widget-holder .grid-divider{
+	clear: both;
+	width: 100%;
+	height: 12px;
+	margin-bottom: 0.75em;
+	margin-top: 0;
+	background: none repeat scroll 0 0 #ffffff;
+	border-bottom: 1px solid #dddddd;
+}
+.widget-holder .rating_stars-sub{clear: both;}
+.widget-holder .close.price{right: -3px; top: -4px;}
+.widget-holder .add_to_cart{margin-top: 4px;}
+.widget-holder .close.add_to_cart{right: 2px;}
+.widget-holder .link-wishlist{margin-right: 2px;}
+.widget-holder .close.wishlist{right: -3px;}
+.widget-content .wishlist-sub{display: inline-block;}
+.widget-content .add_to_cart-sub{clear: both;}
+.widget-content .quickview{border-right: 1px solid #ddd; margin-right: 13px;}
+.widget-content .no-border .quickview{border: none;}
+.widget-content .wishlist{margin-right: 15px;}
+
+.items-container{
+	float: left;
+	padding: 0 0 0 40px;
+	width: 255px;
+}
+.items-container .item{float: left; clear: both; cursor: pointer; margin-bottom: 5px;}
+.items-container .reviews{clear: both;}
+.items-container .moreviews-item,
+.items-container .quickview-item{
+	width: 147px;
+	height: 30px;
+	padding:0;
+	text-align: center;
+}
+.items-container .moreviews-item img,
+.items-container .quickview-item img{
+	padding:8px 30px 7px;
+}
